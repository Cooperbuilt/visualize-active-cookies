{
  "name": "Live Cookies",
  "version": "1.0",
  "author": "Evan Cooper",
  "description": "An extension see your cookies, live!",
  "permissions": [
    "cookies",
    "storage",
    "tabs",
    "http://*/*",
    "https://*/*",
    "activeTab",
    "webRequest"
  ],
  "icons": {
    "16": "icon.png",
    "48": "icon.png",
    "128": "icon.png"
  },
  "browser_action": {
    "default_title": "Live Cookies",
    "default_icon": "icon.png",
    "default_popup": "index.html"
  },
  "manifest_version": 2
}
